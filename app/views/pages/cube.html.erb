<div id="content">
	<div id="tab-types">
		<ul class="nav nav-tabs" data-tabs="tabs">
			<li class="active"><a href="#sugarboard">SugarBoard</a></li>
			<li><a href="#sugarcube">SugarCube</a></li>
			<li><a href="#sugarline">SugarLine</a></li>
			<li><a href="#journal">Journal</a></li>
			<li><a href="#polls">Polls</a></li>
			<li><a href="#sweethearts">SweetHearts</a></li>
		</ul>
	</div>
	<div id="my-tab-content" class="tab-content">
		<div class="tab-pane active" id="sugarboard">
			<%= render 'pages/sugarboard' %>
		</div>
		<div class="tab-pane" id="sugarcube">
			<h1>SugarCube</h1>
			<p>orange orange orange orange orange</p>
		</div>
		<div class="tab-pane" id="sugarline">
			<h1>SugarLine</h1>
			<p>yellow yellow yellow yellow yellow</p>
		</div>
		<div class="tab-pane" id="journal">
			<%= stylesheet_link_tag "/Content/themes/custom-theme/jquery-ui-1.8.16.custom.css" %>
			<%= stylesheet_link_tag "/Content/bootstrap-noicon.css" %>
			<%= stylesheet_link_tag "/Content/font-awesome.css" %>
			<%= stylesheet_link_tag "/Content/site.css" %>
			<%= stylesheet_link_tag "/Content/bootstrap-responsive.css" %>
			<script type="text/javascript" src="/assets/jquery-1.8.1.js"></script>
			<script type="text/javascript" src="/assets/jquery-ui-1.8.23.js"></script>
			<script type="text/javascript" src="/assets/bootstrap.js"></script>
			<script type="text/javascript" src="/assets/jquery.isotope.js"></script>
			<script type="text/javascript" src="/assets/modernizr-2.6.2.js"></script>
			<script type="text/javascript" src="/assets/ckeditor/ckeditor.js"></script>
			<%= render 'journal' %>
		</div>
		<div class="tab-pane" id="polls">
			<h1>Polls</h1>
			<p>blue blue blue blue blue</p>
		</div>
		<div class="tab-pane" id="sweethearts">
			<h1>SweetHearts</h1>
			<p>blue blue blue blue blue</p>
		</div>
	</div>
</div>

<script type="text/javascript" src="http://twitter.github.com/bootstrap/1.4.0/bootstrap-tabs.js"></script>
<script type="text/javascript">
jQuery(document).ready(function ($) {
	$(".tabs").tabs();
});
</script>